<section class="section">
    <h2>Books</h2>
    <div class="container flex flex-wrap justify-between p-8 border-2 border-black">
        {% for book in bookNotes %}
            <div class="book">
                <h2>{{book.fileName}}</p>
                <embed src="/static/pdfs/{{book.trimmedName}}" width="300" height="375"/>
                {# <a href="{{book.link}}">View Notes</a> #}
            </div>
        {% endfor %}
    </div>
</section>